function j = minFuncJac(params,x)
    j = zeros (length(x),length(params));
    delta = zeros(size(params));
    %for i=1:length(x)
        %compute one row of jac
        for k=1:length(params)
            delta(k) = 1e-5;
            j(:,k) = minFunc(params+delta,)-minfunc(params)
        end
        %
    %end
end